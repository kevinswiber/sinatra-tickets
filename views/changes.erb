<changes
  xmlns="urn:org.restfest.2012.hackday.helpdesk.feed"
  xmlns:ticket="urn:org.restfest.2012.hackday.helpdesk.ticket"
  xmlns:comment="urn:org.restfest.2012.hackday.helpdesk.comment"
  xmlns:atom="http://www.w3.org/2005/Atom"
>
  <atom:link rel="self" type="application/vnd.org.restfest.2012.hackday+xml" href="<%= self_url %>?from=2012-09-13T12:01:00Z&amp;to=2012-09-13T12:01:59Z" />

  <!-- TODO: implement prev/next relations -->

  <% changes.each do |row| %>
  <event timestamp="<%= row['created_at'].to_time.iso8601 %>" type="<%= row['change_type'] %>">
    <%= row['payload'] %>
  </event>
  <% end %>
  
  <xhtml:form rel="http://helpdesk.hackday.2012.restfest.org/rels/changes" action="<%= self_url %>" type="application/vnd.org.restfest.2012.hackday+xml" method="get">
    <input type="datetime" name="from" />
    <input type="datetime" name="to" />
  </xhtml:form>
</changes>